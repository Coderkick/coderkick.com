<!DOCTYPE html>
<html lang="en" >
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="manifest" href="/site.webmanifest">
    <title>@ViewData["OGTitle"] - Coderkick</title>
    <meta name="description" content="@ViewData["OGDescription"]">
    <meta name="og:title" content="@ViewData["OGTitle"]">
    <meta name="og:description" content="@ViewData["OGDescription"]">
    <meta name="og:image" content="/assets/og-image.png">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.min.js" integrity="sha384-qKXV1j0HvMUeCBQ+QVp7JcfGl760yU08IQ+GpUo5hlbpg51QRiuqHAJz8+BrxE/N" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter&family=Nunito:wght@200&family=Poppins:wght@400;600&family=Raleway:wght@300&family=Source+Code+Pro:wght@700&family=Space+Grotesk:wght@700&display=swap&family=Space+Mono:wght@700&display=swap" rel="stylesheet">
    <script src=" https://cdn.jsdelivr.net/npm/js-cookie@3.0.1/dist/js.cookie.min.js "></script>
</head>
<body>
        <partial name="_DebugModePartial" />
        <partial name="_Navbar" />
        <div align="center">
        <br><br><br>
        <h1>@ViewData["Title"]</h1>
        <p>@ViewData["Description"]</p>
        <main></main>
            @RenderBody()
        </main>
        <div class="toast-container position-fixed bottom-0 end-0 p-3">
            <partial name="_CookieConsentPartial" />
        </div>
        <a rel="me" hidden href="https://mastodon.social/@@coderkick"></a>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>